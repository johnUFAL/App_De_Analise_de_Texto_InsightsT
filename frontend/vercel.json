{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    { "source": "/favicon.ico", "destination": "/images/logo.png" },
    { "source": "/favicon.png", "destination": "/images/logo.png" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "cleanUrls": true
}
